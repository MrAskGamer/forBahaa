        "sources": [
            "Previous"
        ],
        "originalAnswer": "Right middle lobe pathology",
        "unit": "resp",
        "unitOrder": 3,
        "week": 4,
        "discipline": "Radiology",
        "lectureId": "pNBU7zexYI8JWWMR41ho"
    }
